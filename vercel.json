{
  "redirects": [
    {
      "source": "/",
      "destination": "https://ekatvamcowork.com",
      "permanent": true
    }
  ],
  "routes": [
    {
      "src": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      },
      "dest": "/sitemap.xml"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
